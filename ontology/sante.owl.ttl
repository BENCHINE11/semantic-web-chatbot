@prefix : <http://example.org/sante#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Maladie a owl:Class .
:Symptome a owl:Class .
:Traitement a owl:Class .

:aSymptome a owl:ObjectProperty ;
    rdfs:domain :Maladie ;
    rdfs:range  :Symptome .

:estTraiteePar a owl:ObjectProperty ;
    rdfs:domain :Maladie ;
    rdfs:range  :Traitement .

:Grippe a :Maladie .
:Migraine a :Maladie .

:Fievre a :Symptome .
:Toux   a :Symptome .
:MalDeTete a :Symptome .

:Paracetamol a :Traitement .
:Ibuprofene a :Traitement .

:Grippe :aSymptome :Fievre , :Toux .
:Migraine :aSymptome :MalDeTete .

:Migraine :estTraiteePar :Paracetamol .
:Grippe :estTraiteePar :Ibuprofene .
